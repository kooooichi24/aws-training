{
  "name": "sls-rest-api-notify-error",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "format": "prettier --write .",
    "check-types": "tsc --noEmit",
    "eslint": "eslint '*/**/*.{js,ts}' --quiet",
    "lint": "run-s eslint check-types",
    "lint:fix": "run-s lint format",
    "test:unit": "jest",
    "lint-staged": "lint-staged",
    "build": "tsc -p tsconfig.build.json",
    "start:dynamodb-local": "docker-compose -f ./docker/docker-compose.yml up",
    "offline:start": "sls offline start",
    "deploy:local": "run-p start:dynamodb-local offline:start",
    "deploy:dev": "sls deploy --stage dev",
    "remove:dev": "sls remove --stage dev"
  },
  "author": "Koichi Furukawa",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.49.0",
    "dayjs": "^1.10.7",
    "uuidv4": "^6.2.12"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "^26.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-html-reporters": "^3.0.5",
    "lint-staged": "^12.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "rimraf": "^3.0.2",
    "serverless": "^3.1.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-layers": "^2.5.4-beta.1",
    "serverless-offline": "^8.4.0",
    "serverless-plugin-typescript": "^2.1.1",
    "serverless-prune-plugin": "^2.0.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "*/**/*.{js,ts}": [
      "npm run check-types",
      "npm run eslint",
      "npm run format"
    ]
  }
}
